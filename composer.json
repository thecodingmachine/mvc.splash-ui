{
	"name" : "mouf/mvc.splash-ui",
	"description" : "This package contains the Mouf user interface for Splash",
	"type" : "mouf-library",
	"authors" : [{
			"name" : "David NÃ©grier",
			"email" : "d.negrier@thecodingmachine.com",
			"homepage" : "http://mouf-php.com"
		}
	],
	"keywords" : [
		"mvc",
		"framework",
		"mouf",
		"splash",
		"splash-ui"
	],
	"homepage" : "https://github.com/thecodingmachine/mvc.splash-ui",
	"license" : [
		"MIT"
	],
	"require" : {
		"php" : ">=7.0",
		"laminas/laminas-stratigility": "^3.9",
		"mouf/mvc.splash-common": "^10.0",
		"mouf/mouf": "~2.0.0"
	},
	"autoload" : {
		"psr-4" : {
			"Mouf\\Mvc\\Splash\\" : "src/Mouf/Mvc/Splash"
		}
	},
	"extra" : {
		"mouf" : {
			"require-admin" : [
				"src/SplashUiAdmin.php"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"mouf/mouf-installer": true,
			"mouf/html.utils.weblibrarymanager.component-installer": true,
			"laminas/laminas-dependency-plugin": true
		}
	}
}
